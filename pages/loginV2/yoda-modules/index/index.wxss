.popslider {
    position: fixed;
    width: 100%;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 1;
    background: rgba(0,0,0,0.4);
}

.slider {
    position: fixed;
    width: 600rpx;
    height: 300rpx;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    z-index: 99;
    background: #f2f2f2;
    border-radius: 8rpx;
    padding: 10rpx 10rpx;
}

.slider .slider {
    overflow: hidden;
}

.slider .title {
    margin: 30rpx 0 30rpx;
    font-size: 40rpx;
    height: 34rpx;
    line-height: 34rpx;
    color: #030303;
    text-align: center;
}

.slider .code-warpper {
    text-align: center;
}

.slider .code-warpper .code {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    padding: 0rpx 50rpx;
    margin-bottom: 18rpx;
}

.slider .code-warpper .code .img {
    height: 88rpx;
    width: 210rpx;
    border: 1rpx solid #ccc;
    border-radius: 3rpx;
    margin: 0rpx 10rpx;
}

.slider .code-warpper .code .ipt {
    height: 88rpx;
    width: 220rpx;
    border: 1rpx solid #ccc;
    background-color: #fff;
    color: #333;
    font-size: 32rpx;
    margin: 0rpx 10rpx;
}

.slider .code-warpper .btn {
    border-top: 1rpx solid #06C1AE;
    height: 48rpx;
    line-height: 48rpx;
    padding: 20rpx;
    text-align: center;
    font-size: 32rpx;
    color: #06C1AE;
}

.slider .code-warpper .btn:hover {
    color: rgba(167,224,218,0.45);
}

.slider .close {
    z-index: 9999;
    position: absolute;
    right: 14rpx;
    top: 14rpx;
    background: url("data:image/png;base64,");
    height: 40rpx;
    width: 40rpx;
    background-position: 50% 50%;
    background-size: 32rpx;
    background-repeat: no-repeat;
}

.slider .close:hover {
    background-color: rgba(212,212,212,0.2);
}

.slider-warpper {
    margin: 50rpx auto;
    font-size: 16rpx;
    text-align: center;
}

.slider-warpper .slider-boxWrapper {
    margin: 0 30rpx;
    position: relative;
    height: 100rpx;
    width: 540rpx;
    border: 1rpx solid #cfcfcf;
    background-image: url("data:image/png;base64,");
    background-position: 96% 50%;
    background-repeat: no-repeat;
    background-color: #d4d4d4;
    background-size: 32rpx;
    z-index: 1;
    z-index: 1;
}

.slider-warpper .slider-boxWrapper .tip {
    position: absolute;
    left: 104rpx;
    display: block;
    height: 100rpx;
    line-height: 100rpx;
    color: #999;
    font-size: 32rpx;
    top: 0;
    background-size: 100rpx;
}

.slider-warpper .slider-boxStatic {
    background-image: url("data:image/png;base64,");
}

.slider-warpper .slider-boxError {
    background-image: url("data:image/png;base64,");
}

.slider-warpper .slider-boxLoading {
    background-image: url("data:image/png;base64,");
}

.slider-warpper .slider-boxOk {
    background-image: url("data:image/png;base64,");
}

.slider-warpper .slider-boxStatic,.slider-warpper .slider-boxError,.slider-warpper .slider-boxLoading,.slider-warpper .slider-boxOk,.slider-warpper .slider-box {
    background-position: 50% 50%;
    background-repeat: no-repeat;
    background-color: #fff;
    background-size: 44rpx;
    position: absolute;
    left: 0;
    margin: 0;
    width: 100rpx;
    height: 100rpx;
    z-index: 2;
    cursor: move;
}

.slider-warpper .slider-movingBar {
    position: absolute;
