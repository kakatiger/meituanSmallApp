page {
    background: #eee;
}

.list {
    position: relative;
    width: 100%;
    height: 100%;
}

.list .title {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
    color: #333;
}

.list .row {
    position: relative;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 32rpx;
    padding-left: 20rpx;
    border-bottom: 1rpx solid #ccc;
    color: #000;
    background-color: #FFF;
}

.list .row .icon {
    position: absolute;
    top: 0rpx;
    right: 40rpx;
}