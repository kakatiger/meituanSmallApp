.redpack-entry-container {
    width: 100%;
    height: 274rpx;
    background-image: url('data:image/png;charset=utf-8;base64,');
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    margin-bottom: 30rpx;
    font-family: PingFang-SC-Regular;
}

.full-background-image {
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.redpack-container {
    margin-top: 21rpx;
    width: 678rpx;
    height: 164rpx;
    background-color: #C7472B;
    border-radius: 6rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}

.redpack-content {
    width: 640rpx;
    height: 130rpx;
    background-image: url('data:image/png;charset=utf-8;base64,');
    display: flex;
    flex-flow: row nowrap;
}

.redpack-left-tag {
    width: 156rpx;
    height: 130rpx;
    display: flex;
    flex-flow: column nowrap;
    padding-top: 16rpx;
    box-sizing: border-box;
}

.redpack-value {
    color: #D92700;
    font-size: 60rpx;
    font-family: PingFang-SC-Medium;
    line-height: 60rpx;
    margin-bottom: 5rpx;
    margin-left: 32rpx;
}

.redpack-value-toolong {
    font-size: 44rpx;
    margin-left: 0;
    text-align: center;
}

.redpack-value::before {
    content: 'Â¥';
    font-size: 30rpx;
    font-family: PingFang-SC-Regular;
    line-height: 30rpx;
}

.redpack-condition {
    font-size: 20rpx;
    line-height: 28rpx;
    color: #666;
}

.margin-left-36 {
    margin-left: 36rpx;
}

.margin-left-34 {
    margin-left: 34rpx;
}

.redpack-right-tag {
    display: flex;
    flex-flow: column nowrap;
    padding-top: 22rpx;
}

.redpack-title {
    color: #333;
    font-size: 32rpx;
    line-height: 45rpx;
    margin-left: 34rpx;
    margin-bottom: 15rpx;
    width: 290rpx;
    white-space: nowrap;
    overflow-x: hidden;
    text-overflow: ellipsis;
}

.redpack-red-cover {
    position: absolute;
    top: 0;
    right: 0;
    width: 234rpx;
    height: 164rpx;
    background-image: url('data:image/png;charset=utf-8;base64,');
}

.redpack-button-area {
    position: absolute;
    right: 0;
    top: 0;
    width: 206rpx;
    height: 100%;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
}

.redpack-down-arrow {
    width: 54rpx;
    height: 54rpx;
    margin-top: 31rpx;
}

.redpack-button {
    margin-top: 16rpx;
    font-size: 32rpx;
    line-height: 30rpx;
    color: #FFF2E4;
}

.checkAll {
    padding-top: 25rpx;
    color: #D75539;
    font-size: 24rpx;
    flex-grow: 1;
    text-align: center;
    width: 678rpx;
}

.right-arrow-icon {
    width: 8rpx;
    height: 16rpx;
    margin-left: 8rpx;
}

.redpack-img-container {
    height: 274rpx;
    position: relative;
    margin-bottom: 30rpx;
}

.redpack-img {
    width: 100%;
    height: 100%;
}

.redpack-img-title {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    margin: auto;
    margin-top: 72rpx;
    font-family: PingFang-SC-Medium;
    font-size: 40rpx;
    color: #D15337;
    letter-spacing: 1rpx;
    text-align: center;
}