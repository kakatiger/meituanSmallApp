.poi-container {
    height: 100%;
    width: 100%;
}

.share-wrap {
    overflow: hidden;
}

.bottom-wrap {
    width: 100%;
    height: 120rpx;
}

page {
    height: 100%;
    background: url('http://p1.meituan.net/codeman/5bb215bab41132c458e244942ae04bdf27821.jpg');
}

.section-1-wrap {
    margin: 0 auto;
    width: 690rpx;
    overflow: hidden;
    padding-top: 20rpx;
    padding-bottom: 34rpx;
}

.section-1-txt {
    float: right;
    background: rgba(255,255,255,0.2);
    padding: 10rpx 10rpx 10rpx 30rpx;
    border-radius: 8rpx;
    width: 590rpx;
}

.section-1-txt-0 {
    font-size: 24rpx;
    line-height: 33rpx;
    color: #fff;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-height: 33rpx;
}

.section-1-txt-1 {
    font-size: 28rpx;
    line-height: 42rpx;
    max-height: 84rpx;
    color: #fff;
    width: 590rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.section-1-pic {
    width: 54rpx;
    height: 54rpx;
    border-radius: 50%;
    float: left;
}

.section-2-wrap {
    background: #fff;
    width: 690rpx;
    border-radius: 12rpx;
    margin: 0 auto;
    margin-top: 112rpx;
}

.poi-logo-wrap {
    height: 112rpx;
    position: relative;
}

.poi-logo-bg {
    width: 224rpx;
    height: 224rpx;
    border-radius: 50%;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translate(-50%,0);
    background: #fff;
}

.poi-logo {
    width: 180rpx;
    height: 180rpx;
    position: absolute;
    border-radius: 50%;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    border: 1rpx solid #f4f4f4;
    overflow: hidden;
    background-size: 100% auto;
    background-repeat: no-repeat;
    background-position: center center;
}

.poi-name {
    font-size: 36rpx;
    color: #2C3038;
    margin: 0 auto 12rpx;
    text-align: center;
    font-family: PingFang-SC-Medium;
    width: 440rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.section-2-main {
    background: #fff;
    overflow: hidden;
}

.poi-score-view {
    margin-bottom: 12rpx;
    text-align: center;
}

.poi-score-view image {
    width: 39rpx;
    height: 39rpx;
    border-radius: 8rpx;
    margin-right: 8rpx;
}

.business-fee {
    color: #999;
    font-size: 24rpx;
    line-height: 32rpx;
    text-align: center;
    margin-bottom: 12rpx;
}

.dp-score {
    margin-bottom: 23rpx;
}

.business-privilege {
    border-top: 1rpx dotted #e5e5e5;
    width: 614rpx;
    padding: 30rpx 30rpx 16rpx 0;
    margin: 0 auto;
    position: relative;
}

.discount-item {
    line-height: 34rpx;
    color: #63666B;
    font-size: 28rpx;
    position: relative;
    margin-bottom: 14rpx;
    width: 480rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.discount-item:last-child {
    margin-bottom: 0;
}

.discount-item image {
    margin-right: 20rpx;
    width: 34rpx;
    height: 34rpx;
    vertical-align: bottom;
}

.discount-show-button {
    position: absolute;
    bottom: 30rpx;
    right: 0rpx;
    color: #9B9B9B;
    font-size: 22rpx;
    line-height: 30rpx;
    padding-right: 30rpx;
    min-width: 30rpx;
    min-height: 30rpx;
}

.discount-show-ui {
    width: 20rpx;
    height: 10rpx;
    position: absolute;
    right: 0;
    top: 10rpx;
}

.business-graph {
    position: relative;
    height: 40rpx;
    background: none;
    margin: 0 auto;
    width: 690rpx;
}

.business-graph-middle {
    height: 40rpx;
    width: 650rpx;
    position: absolute;
    left: 20rpx;
    display: inline-block;
    background: #fff;
}

.circle-left {
    width: 30rpx;
    height: 40rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: 30rpx 40rpx;
    display: inline-block;
    position: absolute;
    left: 0;
}

.circle-right {
    width: 30rpx;
    height: 40rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: 30rpx 40rpx;
    display: inline-block;
    transform: rotateY(180deg);
    position: absolute;
    right: 0;
}

.coupon-show {
    width: 110rpx;
    height: 134rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: cover;
    float: left;
    margin-left: 30rpx;
    position: relative;
}

.section-coupon-wrap {
    overflow: hidden;
    background: #fff;
    width: 690rpx;
    margin: 0 auto;
}

.coupon-wrap {
    padding: 40rpx 0;
    border-bottom: 1rpx dotted #e5e5e5;
}

.coupon-credit {
    color: #fff;
    width: 100%;
    text-align: center;
    font-size: 56rpx;
    position: absolute;
    height: 80rpx;
    line-height: 80rpx;
    bottom: 0rpx;
}

.coupon-grab-right {
    float: left;
    margin-left: 18rpx;
    height: 134rpx;
    position: relative;
    width: 500rpx;
}

.coupon-limit {
    color: #2C3038;
    font-size: 28rpx;
    line-height: 40rpx;
}

.coupon-grab {
    height: 80rpx;
    line-height: 80rpx;
    border: 1rpx solid #c7c7c7;
    border-radius: 7rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 500rpx;
}

.coupon-grab-phone {
    height: 80rpx;
    line-height: 80rpx;
    width: 330rpx;
    padding-left: 40rpx;
    font-size: 28rpx;
    color: #aeaeae;
    background: #F7F7F7;
}

.grab-btn {
    width: 130rpx;
    display: inline-block;
    text-align: center;
    line-height: 80rpx;
    height: 80rpx;
    background: #ffd300;
    border-radius: 7rpx;
    font-size: 32rpx;
    color: #6B450A;
    position: absolute;
    right: 0;
    top: 0;
}

.rule-icon {
    width: 32rpx;
    height: 32rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: 32rpx 32rpx;
    margin-left: 14rpx;
    top: 4rpx;
    position: relative;
    display: inline-block;
}

.coupon-item {
    width: 630rpx;
    height: 150rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: 630rpx 150rpx;
    margin: 0 auto;
    margin-bottom: 30rpx;
    position: relative;
}

.coupon-item:last-child {
    margin-bottom: 0;
}

.coupon-suc-title {
    font-size: 36rpx;
    color: #2C3038;
    line-height: 50rpx;
    height: 50rpx;
    text-align: center;
    width: 100%;
}

.coupon-suc-sub-title {
    font-size: 24rpx;
    color: #63666B;
    line-height: 32rpx;
    height: 32rpx;
    text-align: center;
    width: 100%;
    margin-top: 4rpx;
    margin-bottom: 27rpx;
}

.coupon-item-left {
    color: #FB4E44;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    line-height: 100rpx;
    width: 160rpx;
    text-align: center;
}

.coupon-item-left-1 {
    font-size: 48rpx;
}

.coupon-item-left-2 {
    font-size: 84rpx;
    margin-left: 6rpx;
}

.coupon-item-right {
    position: absolute;
    left: 224rpx;
    top: 32rpx;
}

.coupon-item-right-1 {
    font-size: 32rpx;
    line-height: 45rpx;
    margin-bottom: 7rpx;
    color: #353535;
    font-weight: bold;
}

.coupon-item-right-2 {
    font-size: 24rpx;
    color: #63666B;
    line-height: 33rpx;
}

.coupon-result {
    font-size: 24rpx;
    color: #63666B;
    line-height: 32rpx;
    overflow: hidden;
    padding: 0 30rpx;
}

.coupon-result-left {
    float: left;
}

.coupon-result-right {
    float: right;
    padding-right: 30rpx;
    position: relative;
}

.ui-arrow-right {
    width: 19rpx;
    height: 10rpx;
    position: absolute;
    right: 0;
    top: 10rpx;
    transform: rotate(90deg);
}

.coupon-share {
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    border: 1px solid #FB4E44;
    color: #FB4E44;
    font-size: 32rpx;
    border-radius: 8rpx;
    margin: 36rpx 30rpx 50rpx 30rpx;
}

.food-wrap-title {
    font-size: 28rpx;
    color: #4A4A4A;
    text-align: center;
    padding: 22rpx 0;
}

.ui-toast {
    width: 400rpx;
}

.fodd-subject-img {
    width: 196rpx;
    height: 196rpx;
}

.food-subject-wrap {
    float: left;
    margin-right: 20rpx;
    position: relative;
}

.food-subject-wraplast {
    margin-right: 0;
}

.food-subject-container {
    margin: 0 30rpx;
    overflow: hidden;
}

.food-subject-wrap {
    margin-bottom: 30rpx;
}

.food-subject-name {
    color: #2C3038;
    font-size: 28rpx;
    line-height: 40rpx;
    margin-top: 13rpx;
    width: 196rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.food-subject-subname {
    color: #656565;
    font-size: 22rpx;
    line-height: 30rpx;
}

.food-subject-dollar {
    font-size: 22rpx;
    line-height: 40rpx;
    color: #F01800;
}

.food-price {
    font-size: 32rpx;
    line-height: 40rpx;
    color: #F01800;
}

.food-original-price {
    margin-left: 30rpx;
    font-size: 22rpx;
    color: #656565;
    line-height: 30rpx;
    text-decoration: line-through;
}

.sale-info {
    position: absolute;
    top: 0;
    left: 0;
    background: #f01800;
    font-size: 22rpx;
    line-height: 30rpx;
    color: #fff;
    text-align: center;
    width: 196rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.section-coupon-wraplast {
    padding-bottom: 0;
    border-radius: 0 0 12rpx 12rpx;
}

.gotoPoi {
    background: #FFD300;
    position: fixed;
    left: 0;
    bottom: 0;
    line-height: 88rpx;
    text-align: center;
    color: #6B450A;
    font-size: 32rpx;
    width: 100%;
    z-index: 9999;
}

.poi-tip-wrap {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.7);
    z-index: 999;
}

.poi-tip {
    background: #fff;
    margin: 230rpx 40rpx;
    padding: 70rpx 0;
    position: relative;
    border-radius: 10rpx;
}

.poi-tip-title {
    font-size: 32rpx;
    color: #2f2f2f;
    line-height: 45rpx;
    text-align: center;
    margin-bottom: 23rpx;
    width: 100%;
}

.poi-tip-text {
    color: #656565;
    font-size: 24rpx;
    line-height: 36rpx;
    margin: 0 auto;
    width: 600rpx;
}

.poi-close {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 15rpx;
    top: 10rpx;
    background: url('data:image/png;charset=utf-8;base64,');
    background-size: contain;
}

.bottom-bar {
    height: 120rpx;
}

.gray-line {
    width: 600rpx;
    height: 1rpx;
    position: relative;
    top: 50%;
    margin: 0 auto;
    border-bottom: 1rpx dotted #e5e5e5;
}