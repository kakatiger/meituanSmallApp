.mask {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 1001;
}

.toast {
    position: fixed;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    background-color: #FFFFFF;
    width: auto;
    min-height: 310rpx;
    max-height: 750rpx;
    padding: 40.1rpx 0rpx 0rpx 30rpx;
}

.toast-head-text {
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 0.46rpx;
}