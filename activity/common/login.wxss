.captureInputBox {
    position: relative;
}

.captureInputBox .login_mobile_countdown,.captureInputBox .clear-phone {
    position: absolute;
    width: 60rpx;
    height: 40rpx;
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: center;
    right: 60rpx;
    top: 30rpx;
    color: #AEAEAE;
    z-index: 1200;
}

.captureInputBox .clear-phone {
    width: 30rpx;
    height: 30rpx;
    top: 35rpx;
    background: url('data:image/png;charset=utf-8;base64,') no-repeat;
    background-size: 100%;
}

.captureInputBox .login_mobile_countdown.re_get {
    width: 120rpx;
    color: #333;
    font-size: 28rpx;
}

.loginCaptch {
    height: 66rpx;
    width: 200rpx;
    display: block;
    margin: 0 auto 30rpx;
}

.loginMobileTips {
    color: #999999;
    font-size: 24rpx;
    line-height: 33rpx;
    height: 66rpx;
    margin: 0 auto 30rpx;
    width: 480rpx;
}

.loginMobileInput {
    width: 458rpx;
    padding-left: 20rpx;
    margin: 0 auto 10rpx;
    background: #f4f4f4;
    height: 80rpx;
    font-size: 32rpx;
    border-radius: 4rpx;
}

.loginMobileInputBar {
    position: relative;
    margin: 0 auto 10rpx;
    width: 480rpx;
    border: 1rpx solid #C4C4C4;
    height: 80rpx;
}